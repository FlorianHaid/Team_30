<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.SetRoute" parent="Theme.MaterialComponents">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryVariant">@color/primaryDarkColor</item>
        <item name="colorOnPrimary">@color/primaryTextColor</item>
        <item name="android:textColorPrimary">@color/primaryTextColor</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/secondaryColor</item>
        <item name="colorSecondaryVariant">@color/secondaryDarkColor</item>
        <item name="colorOnSecondary">@color/secondaryTextColor</item>
        <item name="android:textColorSecondary">@color/secondaryTextColor</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/primaryDarkColor</item>

        <item name="feedbackTextColor">@color/white</item>
        <!-- Customize your theme here. -->
        <item name="background">@color/grey900</item>
        <item name="colorOnBackground">@color/white</item>
        <item name="colorSurface">@color/grey900</item>
        <item name="colorOnSurface">@color/white</item>
        <item name="android:textColor">@color/white</item>
        <item name="materialButtonStyle">@style/Widget.App.Button</item>
        <item name="feedbackBackground">@color/grey700</item>
    </style>
</resources>